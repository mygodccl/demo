{
  "name": "No Cookies",
  "description": "Removes 'Cookie' and 'Set-Cookie' headers.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}